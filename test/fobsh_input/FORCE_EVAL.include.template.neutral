
&FORCE_EVAL
	METHOD  FIST
	DO_DECOMP T
	&MM
		&FORCEFIELD
			&SPLINE
				RCUT_NB         8
			&END SPLINE
			@IF ${DO_LJ} == 1
			DO_NONBONDED     T
			PARMTYPE CHM
			PARM_FILE_NAME ../topologies/_mol_
			@ENDIF
			@IF ${DO_LJ} /= 1
			DO_NONBONDED     F
			PARMTYPE CHM
			PARM_FILE_NAME ../topologies/_mol_
			@ENDIF
		&END FORCEFIELD
		&POISSON
			&EWALD
				EWALD_TYPE   NONE
			&END EWALD
		&END POISSON
		&PRINT
			&ITER_INFO  SILENT
			&END ITER_INFO
			&PROGRAM_RUN_INFO LOW
				&EACH
					MD 1
				&END EACH
			&END PROGRAM_RUN_INFO
		&END PRINT
	&END MM
	@INCLUDE DECOMP.include
	&SUBSYS
		&VELOCITY
			@INCLUDE VELOC.init
		&END VELOCITY
		@IF ${DO_LJ} /= 1
		@INCLUDE TOPOLOGY-NEUTRAL-ONLY.include
		@ENDIF
		@IF ${DO_LJ} == 1
		@INCLUDE TOPOLOGY-NEUTRAL-ONLY.include
		@ENDIF
	&END SUBSYS
	&PRINT
		&PROGRAM_RUN_INFO OFF
		&END PROGRAM_RUN_INFO
	&END PRINT
&END FORCE_EVAL
